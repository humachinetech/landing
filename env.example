# Copy to backend/.env and fill in values
# Backend (on EC2 or local)
PORT=4000
# Replace with your site URL: https://yourdomain.com or http://<ec2-public-ip>
CORS_ORIGIN=https://yourdomain.com

# MySQL (port 3306)
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=
MYSQL_DATABASE=landing

# Frontend build (only when building for production)
VITE_GRAPHQL_URI=/graphql
